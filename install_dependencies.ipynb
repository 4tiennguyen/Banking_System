    "import subprocess\n",
    "# List of required libraries\n",
    "libraries = [\n",
    "    'pandas',\n",
    "    'pymysql',\n",
    "    'Faker',\n",
    "    'datetime',\n",
    "    'pytz'\n",
    "]\n",
    "# Install each library using pip\n",
    "for library in libraries:\n",
    "    subprocess.run(['pip', 'install', library])\n",
    "\n",
    "print(\"Dependencies installed successfully.\")\n"
   ]
  }
